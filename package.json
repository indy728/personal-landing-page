{
  "name": "indy728.github.io",
  "version": "1.0.0",
  "description": "personal front page",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/*.scss css/style.css -w",
    "devserver": "live-server --browser=firefox",
    "start": "webpack-dev-server --config webpack.dev.js --open",
    "build": "webpack --config webpack.prod.js",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/indy728/indy728.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/indy728/indy728.github.io/issues"
  },
  "homepage": "https://github.com/indy728/indy728.github.io#readme",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "bootstrap": "^4.5.0",
    "concat": "^1.0.3",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jquery": "^3.5.1",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "popper.js": "^1.16.1",
    "postcss-cli": "^4.1.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "bootstrap-social": "^5.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "font-awesome": "^4.7.0"
  }
}
